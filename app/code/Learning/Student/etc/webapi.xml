<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

    <!-- Managing shipping guest information -->
    <route url="/V1/api/student/save" method="POST">
        <service class="Learning\Student\Api\StudentRepositoryInterface" method="save"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>

    <route url="/V1/api/student/search" method="GET">
        <service class="Learning\Student\Api\StudentRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>


</routes>